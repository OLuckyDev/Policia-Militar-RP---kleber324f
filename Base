if game.PlaceId == 7100412026 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Policia Militar [RP] - by SCRIPTKILLER_V2", "Midnight")
    
    -- Teleporte
    local Tab = Window:NewTab("Teleporte")
    local Section = Tab:NewSection("Principal")
    Section:NewButton("Spawn", "Teleportar para o Spawn", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1008.65, 1591.23, -5230.15)
    end)
    Section:NewButton("Loja de Carros", "Teleportar para Loja de carros", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(795.916, 1598.86, -5276.69)
    end)
    Section:NewButton("Loja de Armas", "Teleportar para Loja de armas", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-440.151, 1591.62, -5391.35)
    end)
    local Section = Tab:NewSection("Farm Money")
    Section:NewButton("Cofre Banco", "Teleportar para o cofre", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2058.2, 1595.24, -5201.68)
    end)
    Section:NewButton("Entregar Bolsa", "Teleportar para Fazenda", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4238.83, 1591.79, -4928.86)
    end)
    Section:NewButton("Lixo [Trabalho]", "Teleportar para o lixao", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-20.4663, 1591.37, -4996.85)
    end)
    Section:NewButton("Lojinha [Trabalho]", "Teleportar para loja", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1731.29, 1591.34, -5545.89)
    end)
    local Section = Tab:NewSection("Bases")
    Section:NewButton("Tropa da franca - TDF", "Teleportar para base da TDF", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3920.53, 1638.95, -6909.83)
    end)
    Section:NewButton("Comando Vermelho - CV", "Teleportar para base da CV", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4885.39, 1622.4, -5649.09)
    end)
    Section:NewButton("Choque - CE", "Teleportar para base da CE", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-79.9378, 1605.67, -5293.07)
    end)
    Section:NewButton("Yakuza - YK", "Teleportar para base da YK", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1341.07, 1596.51, -6033.84)
    end)
    Section:NewButton("Comandos - COE", "Teleportar para base da COE", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2872.26, 1591.48, -5150.59)
    end)
    Section:NewButton("Bope - BOPE", "Teleportar para base da BOPE", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3387.98, 1591.26, -5227.96)
    end)
    
    -- Client
    local Tab = Window:NewTab("Client")
    local Section = Tab:NewSection("Poderes")
    Section:NewSlider("Velocidade", "Aumente sua velocidade.", 500, 0, function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)
    Section:NewSlider("Pulo", "Aumente seu pulo.", 500, 0, function(s)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
    end)
